{
  "name": "prayer-tracker",
  "version": "1.0.0",
  "description": "Prayer Tracker Application",
  "main": "start-all.js",
  "scripts": {
    "start": "node start-all.js --dev",
    "start:prod": "cross-env NODE_ENV=production node start-all.js",
    "dev": "node start-all.js --dev",
    "setup": "npm run install-deps && npm run setup-db",
    "install-deps": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "setup-db": "cd backend && npm run setup-db && npm run seed-users",
    "build": "cd frontend && npm run build",
    "backend": "cd backend && npm start",
    "frontend": "cd frontend && npm start",
    "test": "cd backend && npm run test-db",
    "clean": "cd backend && npm run clean-db && npm run seed-users",
    "open": "node scripts/open-browser.js",
    "open-dev": "node scripts/open-browser.js http://13.60.193.171:3000",
    "open-prod": "node scripts/open-browser.js http://13.60.193.171:5000"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/prayer-tracker.git"
  },
  "keywords": [
    "prayer",
    "tracker",
    "mosque",
    "react",
    "nodejs"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.0.3"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3"
  }
}
